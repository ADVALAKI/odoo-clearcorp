<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
         <record id="view_journal_form" model="ir.ui.view">
            <field name="name">view.journal.form</field>
            <field name="model">account.journal</field>
            <field name="type">form</field>
            <field name="inherit_id" ref="account.view_account_journal_form"/>
            <field name="arch" type="xml">
                 <data>
                    <page string="Cash Registers" position="after">
                        <page string="Holding Taxes">
                            <group>
                                <separator colspan="4" string="Required Holding Taxes"/>
                                <field name="withholding_tax_required" nolabel="1"/>
                                <separator colspan="4" string="Optional Holding Taxes"/>
                                <field name="withholding_tax_optional" nolabel="1"/>
                            </group>
                        </page>
                    </page>
                </data>
            </field>
        </record>
    </data>
</openerp>