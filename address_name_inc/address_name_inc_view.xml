<?xml version = "1.0" encoding = "UTF-8"?>
<openerp>
	<data>

		<!--
		======================================
				res.partner address form
		======================================
		-->
		
		<record model = "ir.ui.view" id = "address_name_inc_from_view">
			<field name = "name">res.partner.address.add_from</field>
			<field name="model">res.partner</field>
			<field name="inherit_id" ref="base.view_partner_form"/>
			<field name="arch" type="xml">
				<xpath expr="//field[@name='address']/form/field[@name='email']"
					position="after">
						<field name="age"/>
				</xpath>
			</field>
		</record>
	</data>
</openerp>

